<script>

    $(document).ready(function(){

        $('#btnVerde').attr({
            id: 'btnLogin',
            name: 'btnLogin'
        }).
        html('Login');

        $('#btnLaranja').attr({
            id: 'btnCadastro',
            name: 'btnCadastro'
        }).
        html('Fazer Cadastro');

        $('#btnLogin').click(function(){

            var email = $('#email').val();
            var pass = $('#pass').val();

            if (email == 'undefined' || email == '' || email.length < 3) {
                alert('email');
                return false;
            }
            if (pass == 'undefined' || pass == '' || pass.length < 3) {
                alert('Defina uma Senha');
                return false;
            } 
        });

    });

</script>