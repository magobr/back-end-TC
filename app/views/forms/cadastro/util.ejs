<script>

$(document).ready(function(){
    
        $('#idade').mask('99');

        $('#btnVerde').
        attr({
            id: 'btnCadastro',
            name: 'btnCadastro'
        }).
        html('Cadastrar');

        
        $('#btnVermelho').
        attr({
            id: 'btnCancelar',
            name: 'btnCancelar'
        }).
        html('Cancelar');

        $('#btnCadastro').click(function(){


            var nome = $("#nome").val();
            if (nome == 'undefined' || nome == '' || nome.length < 3) {
                alert('nome');
                return false;
            }

            var email = $('#email').val();
            if (email == 'undefined' || email == '' || email.length < 3) {
                alert('email');
                return false;
            }

            var idade = $('#idade').val();
            if (idade == 'undefined' || idade == '') {
                alert('Idade');
                return false;
            } 

            var senha = $('#senha').val();
            if (senha == 'undefined' || senha == '' || senha.length < 3) {
                alert('Defina uma Senha');
                return false;
            }

            var reSenha = $('#reSenha').val();
            if ( reSenha != senha || reSenha == 'undefined' || reSenha == '' || reSenha.length < 3){
                alert('Defina uma Senha');
                return false;
            }

        });

    });
    
</script>