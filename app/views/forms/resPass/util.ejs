<script>

    $(document).ready(function(){

        $('#btnVerde').attr({
            id: 'btnAlter',
            name: 'btnAlter'
        }).
        html('Alterar Senha');

        $('#btnVermelho').attr({
            id: 'btnCancelar',
            name: 'btnCancelar'
        }).
        html('Cancelar');

        $('#btnAlter').click(function(){

            var email = $('#email').val();
            var pass = $('#pass').val();
            var rePass = $('#rePass').val();

            if (email == 'undefined' || email == '' || email.length < 3) {
                alert('email');
                return false;
            }

            if (pass == 'undefined' || pass == '' || pass.length < 3) {
                alert('Defina uma Senha');
                return false;
            }

            if (rePass != pass) {    
                alert('Defina uma Senha');
                return false;
            }
            
            if (rePass == 'undefined' || rePass == '' || rePass.length < 3) {
                alert('Defina uma Senha');
                return false;
            }  
        });

    });

</script>