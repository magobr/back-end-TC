<script>

    $(document).ready(function(){

        $('#btnVerde').attr({
            id: 'btnAlter',
            name: 'btnAlter'
        }).
        html('Alterar Senha');

        $('#btnVermelho').attr({
            id: 'btnCancelar',
            name: 'btnCancelar'
        }).
        html('Cancelar');

        $('#btnAlter').click(function(){

            var email = $('#email').val();
            var senha = $('#senha').val();
            var reSenha = $('#reSenha').val();

            if (email == 'undefined' || email == '' || email.length < 3) {
                alert('email');
                return false;
            }

            if (senha == 'undefined' || senha == '' || senha.length < 3) {
                alert('Defina uma Senha');
                return false;
            }

          
            if (reSenha == 'undefined' || reSenha == '' || reSenha.length < 3) {
                if (reSenha != senha) {    
                    alert('Defina uma Senha');
                    return false;
                }
                alert('Defina uma Senha');
                return false;
            }  
        });

    });

</script>